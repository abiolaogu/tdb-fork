# LumaDB Multi-Protocol Server Configuration

# Server
PORT=8000
BIND_ADDRESS=0.0.0.0

# Protocols (set to 'false' to disable)
ENABLE_DYNAMODB=true
ENABLE_D1=true
ENABLE_TURSO=true

# DynamoDB Configuration
DYNAMODB_REGION=us-east-1
DYNAMODB_AUTH=false

# D1 Configuration
D1_ACCOUNT_ID=lumadb-account
D1_AUTH=false

# Turso Configuration
TURSO_ORG=lumadb-org
TURSO_AUTH=false

# Logging
RUST_LOG=info

# Features
ENABLE_CORS=true
ENABLE_TRACING=true
