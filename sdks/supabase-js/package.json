{
    "name": "@lumadb/supabase-js",
    "version": "0.1.0",
    "description": "Supabase-compatible client for LumaDB",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "test": "vitest run",
        "lint": "eslint src --ext .ts"
    },
    "dependencies": {},
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0",
        "eslint": "^8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0"
    },
    "keywords": [
        "supabase",
        "lumadb",
        "database",
        "client",
        "postgresql"
    ],
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/abiolaogu/LumaDB"
    }
}