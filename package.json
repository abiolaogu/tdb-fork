{
  "name": "lumadb",
  "version": "1.0.0",
  "description": "LumaDB - High-performance distributed database, with multiple query languages and enterprise features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lumadb": "./dist/cli/index.js",
    "luma-server": "./dist/server/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "server": "node dist/server/index.js",
    "dev": "ts-node src/cli/index.ts",
    "dev:server": "ts-node src/server/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "database",
    "sql",
    "nosql",
    "query",
    "storage",
    "acid",
    "transactions",
    "indexing"
  ],
  "author": "TDB+ Contributors",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "readline-sync": "^1.4.10",
    "uuid": "^9.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/readline-sync": "^1.4.8",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}